                    android:gravity="center"
                    android:textColor="@color/textColor"
                    android:textStyle="bold"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="5dp"
                    app:layout_constraintTop_toBottomOf="@id/tvMetalTemp"
