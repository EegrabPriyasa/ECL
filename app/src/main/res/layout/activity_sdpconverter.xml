                    android:textColor="@color/black"
                    android:layout_marginStart="50dp"
                    android:layout_marginTop="5dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/tvMetalQty" />

