    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TabScreen.Converter.SDPConverterActivity">
    <androidx.cardview.widget.CardView
        android:id="@+id/cardTreatmentInfo"
        android:layout_width="900dp"
        android:layout_height="wrap_content"
        app:cardCornerRadius="12dp"
        app:cardElevation="4dp"
        android:layout_marginTop="30dp"
        app:layout_constraintEnd_toStartOf="@+id/finalComposition"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/includeSystemSetup"
        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <!-- Header -->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:background="#4285F4"
                android:gravity="center_vertical"
                android:paddingHorizontal="12dp">

                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@drawable/settings" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:text="Treatment Information"
                    android:textStyle="bold"
                    android:textColor="@android:color/white"
                    android:textSize="16sp" />
            </LinearLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/tvDept"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Dept:"
                    android:textSize="16sp"
                    android:textColor="@color/black"
                    android:layout_marginStart="50dp"
                    android:layout_marginTop="30dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
                <TextView
                    android:id="@+id/tvDeptValue"
                    android:layout_width="140dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/button_bg_converter"
                    android:backgroundTint="@color/blue"
                    android:padding="8dp"
                    android:text="MP"
                    android:gravity="center"
                    android:textColor="@android:color/white"
                    android:textStyle="bold"
                    android:layout_marginStart="50dp"
                    android:layout_marginTop="5dp"
                    app:layout_constraintTop_toBottomOf="@id/tvDept"
                    app:layout_constraintStart_toStartOf="parent"/>
                <TextView
                    android:id="@+id/tvTrtNo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Trt No:"
                    android:textSize="16sp"
                    android:textColor="@color/black"
                    android:layout_marginStart="135dp"
                    android:layout_marginTop="30dp"
                    app:layout_constraintStart_toEndOf="@id/tvDept"
                    app:layout_constraintTop_toTopOf="parent"/>
                <TextView
                    android:id="@+id/tvTrtNoValue"
                    android:layout_width="140dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/button_bg_converter"
                    android:backgroundTint="@color/blue"
                    android:padding="8dp"
                    android:text="1"
                    android:gravity="center"
                    android:textColor="@android:color/white"
                    android:textStyle="bold"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="5dp"
                    app:layout_constraintTop_toBottomOf="@id/tvTrtNo"
                    app:layout_constraintStart_toEndOf="@id/tvDeptValue"/>
                <TextView
                    android:id="@+id/tvMetalForm"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Metal Form:"
                    android:textSize="16sp"
                    android:textColor="@color/black"
                    android:layout_marginStart="125dp"
                    android:layout_marginTop="30dp"
                    app:layout_constraintStart_toEndOf="@id/tvTrtNo"
                    app:layout_constraintTop_toTopOf="parent"/>
